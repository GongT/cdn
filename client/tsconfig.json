{
	"compileOnSave": true,
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"lib": ["es5", "es6", "es7", "dom", "es2015.promise", "es2015.proxy"],
		"moduleResolution": "node",
		"declaration": true,
		"noEmitOnError": true,
		"noImplicitThis": true,
		"noImplicitAny": false,
		"removeComments": false,
		"preserveConstEnums": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": false,
		"pretty": false,
		"sourceMap": true,
		"sourceRoot": "/public/client/",
		"baseUrl": "./",
		"outDir": "../dist/client-code",
		"rootDir": "./",
		"paths": {
			"@gongt/ts-stl-client": ["../node_modules/@gongt/ts-stl-client"],
			"@gongt/ts-stl-library": ["../node_modules/@gongt/ts-stl-library"]
		}
	}
}
